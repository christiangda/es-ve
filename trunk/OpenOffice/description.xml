<?xml version="1.0" encoding="UTF-8"?>
<description xmlns="http://openoffice.org/extensions/description/2006" xmlns:d="http://openoffice.org/extensions/description/2006"  xmlns:xlink="http://www.w3.org/1999/xlink">
	<version value="${PKG_oOo_VERSION}" />
	<platform value="all" />
    <dependencies>
        <OpenOffice.org-minimal-version value="3.0" d:name="OpenOffice.org 3.0."/>
    </dependencies>
	<!--If you uploadet your extension to the repository (which should be the default!) 
        you do not need to have this one.
		http://specs.openoffice.org/appwide/packagemanager/online_update_for_extensions.odt
    -->
    <update-information>
        <src xlink:href="http://es-ve.sourceforge.net" />
    </update-information>
    <registration>
        <simple-license accept-by="admin" suppress-on-update="false" >
            <license-text xlink:href="license/LICENSE-en" lang="en" />
            <license-text xlink:href="license/LICENSE-es" lang="es" />
        </simple-license>
    </registration>
    <identifier value="org.openoffice.dictionaries.es.ve" />
	<display-name>
		<name lang="es">Diccionario de correción ortográfica en español para Venezuela</name>
		<name lang="en">Spanish (Venezuela) spell check dictionary</name>
	</display-name>
    <publisher>
        <name xlink:href="http://es-ve.sourceforge.net" lang="es">Proyecto es-ve</name>
    </publisher>
    <release-notes>
        <src xlink:href="http://es-ve.sourceforge.net" lang="es" />
    </release-notes>
	<icon>
		<default xlink:href="images/icon_venezuela_flag.png" />
		<high-contrast xlink:href="images/icon_venezuela_flag.png" />
	</icon>
	<extension-description>
		<src xlink:href="description/description-en.txt" lang="en" />
		<src xlink:href="description/description-es.txt" lang="es" />
	</extension-description>
</description>
